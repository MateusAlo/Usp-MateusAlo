#include <stdio.h>
#include <math.h>

//Mateus Alonso 13695553 dia 29/04
//Ler x e y e depois exibir a soma dos números de x a y. Incluir os valores de x e y na soma.
int main()
{


    int x, y, z, w,  xi, yi;


    printf(" Digite dois valores: ");
    scanf("%d, %d", &x, &y);


    xi = x;
    yi = y;
    //Enquanto x é menor que y x é somado á variável z e acrescido de 1
    //Depois y é a adicionado á z formando a soma
    while (x < y)
    {

        z = z + x;
        x = x + 1;
    }
    w = z + y;
    printf("Os valores sao: %d, %d ", xi, yi );
    printf("A soma dos valores de x a y: %d", w);



    return 0;
}
