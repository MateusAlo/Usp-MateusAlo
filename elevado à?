#include <stdio.h>
#include <stdio.h>
//Mateus Alonso 13695553 03/05/22

int main(int argc, const char * argv[])
{
    int x, y;
printf("Informe valores para x e y: ");
scanf("%d %d", &x, &y);

int power = 1;
for (int i = 1; i <= y; i++){

    power = power *x;

}

printf("O valor de x^y eh: %d", power);


return 0;
}
